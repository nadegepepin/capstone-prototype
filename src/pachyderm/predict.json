{
  "pipeline": {
    "name": "predict"
  },
  "description": "Predict the caption of a given image.",
  "transform": {
    "cmd": [ "python3", "pachyderm/pachydermtest.py", "--stage=predict"],
    "image": "npepin/capstone-prototype:d"
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "repo": "savedmodel",
          "glob": "/*"
        }
      },
     {
       "pfs": {
         "repo": "inpredict",
         "glob": "/*"
       }
     }
   ]
 },
"parallelism_spec": {
    "constant": 1
  },
 "enable_stats": true
}
