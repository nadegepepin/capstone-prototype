{
  "pipeline": {
    "name": "model"
  },
  "description": "Training of the model.",
  "transform": {
    "cmd": [ "python3", "pachyderm/pachydermtest.py", "--stage=train_model", "--modeldir=/pfs/model"],
    "image": "npepin/capstone-prototype:8"
  },
  "input": {
    "pfs": {
      "repo": "consolidate",
      "glob": "/"
    }
  },
"parallelism_spec": {
    "constant": 1
  },
 "enable_stats": true
}
